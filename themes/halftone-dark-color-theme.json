  "semanticHighlighting": true,

  "semanticTokenColors": {
    "function": "#ededed",
    "method": "#ededed",
    "class": "#ededed",
    "macro": "#ededed",

    "variable": "#CCC",
    "keyword": "#CCC",
    "regexp": "#CCC",
    "operator": "#CCC",
    "enumMember": "#CCC",
    "decorator": "#CCC",
    "parameter": "#CCC",
    "typeParameter": "#CCC",
    "event": "#CCC",
    "label": "#CCC",

    "struct": "#CCC",
    "interface": "#CCC",
    "type": "#CCC",
    "namespace": "#CCC",
    "enum": "#CCC",
    "property": "#CCC",

    "number": "#acacac",
    "string": "#acacac",

    "comment": "#7f7f7f"
  },

  "tokenColors": [
    // Functions
    {
      "scope": "entity.name.function",
      "settings": { "foreground": "#ededed", "fontStyle": "bold" }
    },
    {
      "scope": "support.function",
      "settings": { "foreground": "#ededed", "fontStyle": "bold" }
    },
    // Method
    {
      "scope": "entity.name.method",
      "settings": { "foreground": "#ededed", "fontStyle": "bold" }
    },
    // HTML Tag
    {
      "scope": "entity.name.tag",
      "settings": { "foreground": "#ededed", "fontStyle": "bold" }
    },
    {
      "scope": "support.class.component.astro",
      "settings": {
        "foreground": "#ededed",
        "fontStyle": "bold"
      }
    },

    // Variable
    {
      "scope": "variable",
      "settings": { "foreground": "#CCC", "fontStyle": "" }
    },
    // Type
    {
      "scope": "entity.name.type",
      "settings": { "foreground": "#bcbcbc", "fontStyle": "underline" }
    },
    {
      "scope": "support.type",
      "settings": { "foreground": "#bcbcbc", "fontStyle": "underline" }
    },
    {
      "scope": "storage.modifier",
      "settings": { "foreground": "#bcbcbc", "fontStyle": "italic" }
    },
    // function, const, let, ...
    {
      "scope": "storage.type",
      "settings": { "foreground": "#CCC", "fontStyle": "italic" }
    },
    // return, for, break, ...
    {
      "scope": "keyword.control",
      "settings": { "foreground": "#CCC", "fontStyle": "bold" }
    },
    {
      "scope": "meta.property-name.css",
      "settings": { "foreground": "#CCC" }
    },
    // CSS Selectors (Classes, IDs)
    {
      "scope": "entity.other.attribute-name.class.css",
      "settings": { "foreground": "#CCC", "fontStyle": "bold" }
    },
    {
      "scope": "entity.other.attribute-name.id.css",
      "settings": { "foreground": "#CCC", "fontStyle": "bold" }
    },

    // HTML Tag Attibute name
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#acacac" }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": { "foreground": "#acacac", "fontStyle": "italic" }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#acacac", "fontStyle": "italic" }
    },
    // Numbers
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#acacac" }
    },
    // Strings
    {
      "scope": "string",
      "settings": { "foreground": "#acacac", "fontStyle": "italic" }
    },
    // Arrows
    {
      "scope": "storage.type.function.arrow",
      "settings": { "foreground": "#acacac", "fontStyle": "bold" }
    },
    // Braces
    {
      "scope": "meta.brace",
      "settings": { "foreground": "#8e8e8e" }
    },
    // Equal signs
    {
      "scope": "keyword.operator.assignment",
      "settings": {"foreground": "#8e8e8e" }
    },
    // ! and &&
    {
      "scope": "keyword.operator.logical",
      "settings": {"foreground": "#8e8e8e" }
    },
    {
      "scope": "keyword.operator.type",
      "settings": {"foreground": "#8e8e8e" }
    },
    // Dots
    {
      "scope": "punctuation.accessor",
      "settings": { "foreground": "#8e8e8e" }
    },
    {
      "scope": "punctuation.definition",
      "settings": { "foreground": "#8e8e8e" }
    },
    {
      "scope": "punctuation.terminator.statement",
      "settings": { "foreground": "#8e8e8e" }
    },
    // Comment
    {
      "scope": "comment",
      "settings": { "foreground": "#7f7f7f", "fontStyle": "italic" }
    }
  ]
}
